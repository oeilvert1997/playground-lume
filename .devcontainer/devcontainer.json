{
  "build": {
    "dockerfile": "Dockerfile",
    "context": "."
  },
  "features": {},
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.defaultProfile.linux": "zsh",
        "terminal.integrated.profiles.linux": {
          "zsh": {
            "path": "/bin/zsh",
            "icon": "terminal-zsh"
          }
        },
        "deno.enable": true,
        "deno.path": "/usr/local/bin/deno"
      },
      "extensions": [
        "denoland.vscode-deno"
      ]
    }
  },
  "postCreateCommand": {}
}